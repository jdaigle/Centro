<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="NorthwindTests.Model"
                   assembly="NorthwindTests">
  <class name="Category"
         table="Categories">
    <id name="Id"
        column="CategoryId"
        type="Int32"
        unsaved-value="-1">
      <generator class="native" />
    </id>
    <property name="Name"
              column="CategoryName"
              type="string"
              length="15"
              not-null="true" />
    <property name="Description"
              column="Description"
              type="string" />
    <property name="Picture"
              column="Picture"
              type="binary" />
    <set name="Products"
         access="noop">
      <key column="CategoryId" />
      <one-to-many class="Product" />
    </set>
  </class>
</hibernate-mapping>